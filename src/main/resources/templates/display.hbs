{{#partial "content"}}

<h1>List of Jobs:</h1>
    {{#if allJobs}}
        {{#each allJobs}}
            <p>Company: {{company}}</p>
            <p>Job Title: {{jobTitle}}</p>
            <p>Job Description: {{jobDescription}}</p>
            <p>Date started: {{dateStarted}}</p>
            <p>Date ended: {{dateEnded}}</p>
        {{/each}}
    {{else}}
    <p>You have no career. You are a loser.</p>
    {{/if}}

{{/partial}}

{{> layout.hbs}}